{
  "name": "proto-leaf",
  "version": "1.0.2",
  "description": "prototyping with sheets",
  "main": "index.js",
  "standard": {
    "parser": "babel-eslint"
  },
  "scripts": {
    "prebuild": "rimraf dist && rimraf .cache",
    "start": "parcel src/index.html",
    "dev": "npm start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "parcel build src/index.html --out-dir dist",
    "deploy": "npm run build && firebase deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fbosch/proto-leaf.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fbosch/proto-leaf/issues"
  },
  "homepage": "https://github.com/fbosch/proto-leaf#readme",
  "browserslist": [
    "last 3 chrome versions"
  ],
  "cache": {
    "strategy": "inject",
    "clearDist": true,
    "globDirectory": "./dist",
    "globPatterns": [
      "**/*.{html,js,css,jpg,png,gif,svg,eot,ttf,woff,woff2}"
    ],
    "swSrc": "./src/service-worker.js",
    "templatedURLs": {
      "/": [
        "index.html"
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@svgr/parcel-plugin-svgr": "^5.0.1",
    "babel-eslint": "^10.0.3",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-semantic-ui-react-imports": "^1.4.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^6.8.0",
    "firebase-cli": "^1.2.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "parcel-plugin-sw-cache": "^0.3.1",
    "rimraf": "^3.0.0",
    "sass": "^1.25.0",
    "standard": "^14.3.1"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "dompurify": "^2.0.7",
    "get-youtube-id": "^1.0.1",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.15",
    "md5": "^2.2.1",
    "modern-normalize": "^0.6.0",
    "query-string": "^6.10.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-lite-youtube-embed": "^0.2.32",
    "react-router-dom": "^5.1.2",
    "react-router-scroll-memory": "^2.0.5",
    "semantic-ui-react": "^0.88.2"
  }
}
